bow_macroRef =
  ## points to the specification for some pattern which is to be included in a schema
  element ns1:macroRef {
    empty,
    bow_att.global.attributes,
    bow_att.readFrom.attributes,
    ## the identifier used for the required pattern within the
    ##	source indicated.
    attribute key { xsd:NCName },
    empty
  }

bow_moduleRef =
  ## (module reference) references a module which is to be incorporated into a schema.
  element ns1:moduleRef {
    (bow_content?)
    >> sch:pattern [
         id = "moduleRef-constraint-not-key-and-url"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "                "
           sch:report [
             test = "@url and @key"
             "\x{a}" ~
             "                  only the key= OR the url= attribute of moduleRef should be supplied, not both\x{a}" ~
             "                "
           ]
           "\x{a}" ~
           "              "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-not-include-and-except"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "                "
           sch:report [
             test = "@except and @include"
             "\x{a}" ~
             "                  only the exclude= OR the include= attribute of moduleRef should be supplied, not both\x{a}" ~
             "                "
           ]
           "\x{a}" ~
           "              "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "moduleRef-constraint-modref"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:moduleRef"
           "\x{a}" ~
           "               "
           sch:report [
             test = "* and @key"
             "\x{a}" ~
             "	  child elements of moduleRef are only allowed when an external module\x{a}" ~
             "	  is being loaded\x{a}" ~
             "	"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
