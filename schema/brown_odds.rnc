bow_equiv =
  ## (equivalent) specifies a component which is considered equivalent to the parent element, either by
  ##     co-reference, or by external link.
  element ns1:equiv {
    empty,
    bow_att.global.attributes,
    bow_att.internetMedia.attributes,
    ## names the underlying concept of which the parent is a representation
    attribute name { bow_data.name }?,
    ## (uniform resource identifier) references the underlying concept of which the parent is a representation by means of
    ##         some external identifier
    attribute uri { bow_data.pointer }?,
    ## references an external script which contains a method to transform instances of this
    ##         element to canonical TEI 
    attribute filter { bow_data.pointer }?,
    empty
  }

bow_altIdent =
  ## (alternate identifier) supplies the recommended XML name for an element, class,
  ##   attribute, etc. in some language.
  element ns1:altIdent {
    text
    >> sch:pattern [
         id = "altIdent-constraint-altIdent-only-NCName"
         "\x{a}" ~
         "            "
         sch:rule [
           "\x{a}" ~
           "                "
           sch:assert [
             test =
               "(  parent::tei:attDef       or parent::tei:classSpec       or parent::tei:constraintSpec       or parent::tei:elementSpec       or parent::tei:macroSpec       or parent::tei:moduleSpec       or parent::tei:schemaSpec       ) and       (normalize-space(.) castable as xs:NCName)"
             "\x{a}" ~
             "		  The content of altIdent should be an XML Name\x{a}" ~
             "		  (w/o a namespace prefix), unless a child of\x{a}" ~
             "		  valItem (and even then, it's not a bad idea :-)"
           ]
           "\x{a}" ~
           "              "
         ]
         "\x{a}" ~
         "         "
       ],
    bow_att.global.attributes,
    bow_att.typed.attributes,
    empty
  }

bow_content =
  ## (content model) contains the text of a declaration for the schema
  ## documented.
  element ns1:content {
    bow_macro.schemaPattern*,
    bow_att.global.attributes,
    ## controls whether or not pattern names generated in the
    ## corresponding RELAXNG schema source are automatically prefixed to
    ## avoid potential nameclashes. 
    [ a:defaultValue = "true" ]
    attribute autoPrefix {
      ## Each name referenced in e.g. a rng:ref
      ##	  element within a content model is automatically prefixed by
      ##	  the value of the prefix attribute on the current
      ##	  schemaSpec
      "true" | ## No prefixes are added: 
        ##  any prefix required by the value of the prefix attribute on the current
        ##	  schemaSpec must therefore be supplied explicitly, as appropriate. 
        "false"
    }?,
    empty
  }

bow_constraint =
  ##  (constraint rules) the formal rules of a constraint
  element ns1:constraint {
    (sch_assert | sch_ns | sch_report | sch_rule)+,
    bow_att.global.attributes,
    empty
  }

bow_constraintSpec =
  ##  (constraint on schema) contains  a constraint, expressed in some formal syntax,
  ##   which cannot be expressed in the structural content model
  element ns1:constraintSpec {
    bow_model.glossLike*,
    (bow_constraint?)
    >> sch:ns [
         prefix = "s"
         uri = "http://www.ascc.net/xml/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-sch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/s:* and    not(@scheme='schematron')"
             "\x{a}" ~
             "	Rules in the Schematron 1.* language must be inside\x{a}" ~
             "	a constraint with a value of 'schematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:ns [
         prefix = "sch"
         uri = "http://purl.oclc.org/dsdl/schematron"
       ]
    >> sch:pattern [
         id = "constraintSpec-constraint-isosch"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/sch:* and    not(@scheme='isoschematron')"
             "\x{a}" ~
             "	Rules in the ISO Schematron language must be inside\x{a}" ~
             "	a constraint with a value of 'isoschematron' on the scheme attribute\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    bow_att.global.attributes,
    bow_att.identified.attributes,
    bow_att.typed.attributes,
    ## supplies the name of the language in which the constraints
    ##       are defined
    attribute scheme {
      ##  (Schematron) 
      "schematron" | ##  (ISO Schematron) 
        "isoschematron" | ##  (XSLT) 
        "xsl" | ##  (private constraint language) 
        "private"
    },
    empty
  }

bow_attList =
  ## contains documentation for all the attributes associated with this element, as a series of
  ##       attDef elements.
  element ns1:attList {
    (bow_attRef | bow_attDef | bow_attList)+,
    bow_att.global.attributes,
    ## (organization) specifies whether all the attributes in the list are available (org="group") or only one
    ##         of them (org="choice")
    [ a:defaultValue = "group" ]
    attribute org {
      ## grouped
      "group" | ## alternated
        "choice"
    }?,
    empty
  }

bow_attDef =
  ## (attribute definition) contains the definition of a single attribute.
  element ns1:attDef {
    (bow_model.glossLike*,
     bow_datatype?,
     bow_constraintSpec*,
     bow_defaultVal?,
     (bow_valList | bow_valDesc+)?,
     bow_exemplum*,
     bow_remarks*),
    bow_att.global.attributes,
    bow_att.identified.attributes,
    ## specifies the optionality of an attribute or element. 
    [ a:defaultValue = "opt" ]
    attribute usage {
      ## (required) 
      "req" | ## (mandatory when applicable ) 
        "mwa" | ## (recommended ) 
        "rec" | ## (recommended when applicable ) 
        "rwa" | ## (optional ) 
        "opt"
    }?,
    ## (namespace) specifies the namespace to which this attribute belongs
    [ a:defaultValue = "http://www.tei-c.org/ns/1.0" ]
    attribute ns { bow_data.namespace }?,
    empty
  }

bow_attRef =
  ## (attribute pointer) points to the definition of an attribute or group of attributes.
  element ns1:attRef {
    empty,
    bow_att.global.attributes,
    ## the name of the pattern defining the attribute(s)
    attribute name { bow_data.word },
    empty
  }

bow_datatype =
  ## specifies the declared value for an attribute, by referring to
  ##   any datatype defined by the chosen schema language.
  element ns1:datatype {
    bow_macro.schemaPattern*,
    bow_att.global.attributes,
    ## (minimum number of occurences) indicates the minimum number of times this datatype may
    ##       occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ] attribute minOccurs { bow_data.count }?,
    ## (maximum number of occurences) indicates the maximum number of times this datatype may
    ##       occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ]
    attribute maxOccurs { bow_data.count | "unbounded" }?,
    empty
  }

bow_defaultVal =
  ## (default value) specifies the default declared value for an attribute.
  element ns1:defaultVal { text, bow_att.global.attributes, empty }

bow_valDesc =
  ## (value description) specifies any semantic or syntactic constraint on the value that
  ## an attribute may take, additional to the information carried by the
  ## datatype element.
  element ns1:valDesc {
    bow_macro.phraseSeq.limited,
    bow_att.global.attributes,
    bow_att.translatable.attributes,
    bow_att.combinable.attributes,
    empty
  }

bow_valItem =
  ## documents a single attribute-value within a list of possible
  ##   or mandatory items.
  element ns1:valItem {
    bow_model.glossLike*,
    bow_att.global.attributes,
    bow_att.combinable.attributes,
    ## specifies the attribute value concerned.
    attribute ident { text },
    empty
  }

bow_valList =
  ## (value list) contains one or more valItem elements defining possible values for an attribute.
  element ns1:valList {
    bow_valItem*,
    ## specifies the extensibility of the list of attribute values specified. 
    [ a:defaultValue = "open" ]
    attribute type {
      ## only the values specified are permitted.
      "closed" | ## (semi-open) all the values specified should be supported, but other values are legal and
        ##             software should have appropriate fallback processing for them. 
        "semi" | ## the values specified are sample values only.
        "open"
    },
    bow_att.global.attributes,
    bow_att.combinable.attributes,
    empty
  }
bow_att.combinable.attributes = bow_att.combinable.attribute.mode
bow_att.combinable.attribute.mode =
  ## specifies the effect of this declaration on its parent
  ##     object.
  [ a:defaultValue = "add" ]
  attribute mode {
    ## this declaration is added to the current definitions
    "add" | ## if present already, the whole of the declaration 
      ##	  for this object is  removed from the current setup
      "delete" | ## this declaration changes the declaration of the same
      ##     name in the current definition
      "change" | ## this declaration replaces the declaration of the same
      ##     name in the current definition
      "replace"
  }?
bow_att.identified.attributes =
  bow_att.combinable.attributes,
  bow_att.identified.attribute.module,
  bow_att.identified.attribute.ident,
  bow_att.identified.attribute.predeclare,
  bow_att.identified.attribute.status

bow_att.identified.attribute.module =
  attribute module {
    "analysis" | "certainty" | "gaiji" | "core" | "header" | "textstructure" | "dictionaries" | "tagdocs" | "iso-fs" | "nets" | "linking" | "msdescription" | "corpus" | "namesdates" | "drama" | "tei" | "figures" | "textcrit" | "spoken" | "transcr" | "verse"
  }?
bow_att.identified.attribute.ident =
  ## Supplies the identifier by which this element may be referenced.
  attribute ident { bow_data.name }
bow_att.identified.attribute.predeclare =
  ## Says whether this object should be predeclared in the
  ##       tei infrastructure module.
  [ a:defaultValue = "false" ]
  attribute predeclare { bow_data.truthValue }?
bow_att.identified.attribute.status =
  ## indicates the current status of the object identified with
  ## respect to the current version of the TEI Guidelines.
  [ a:defaultValue = "stable" ]
  attribute status {
    ## the item is not recommended for use,
    ## and may be withdrawn at a future release.
    "deprecated" | ## the item is new and still under
      ## review.
      "unstable" | ## the item has changed significantly since the
      ## preceding version.
      "changed" | ## the item has not recently changed and is
      ## not expected to do so except for correction of any errors.
      "stable"
  }?

bow_persName =
  ## (personal name) contains a proper noun or proper-noun phrase referring to a person, possibly including any
  ##     or all of the person's forenames, surnames, honorifics, added names, etc.
  element ns1:persName {
    bow_macro.phraseSeq,
    bow_att.global.attributes,
    bow_att.datable.attributes,
    bow_att.editLike.attributes,
    bow_att.personal.attributes,
    bow_att.typed.attributes,
    empty
  }

bow_placeName =
  ## contains an absolute or relative place name. 
  element ns1:placeName {
    bow_macro.phraseSeq,
    bow_att.global.attributes,
    bow_att.naming.attributes,
    bow_att.typed.attributes,
    bow_att.datable.attributes,
    bow_att.editLike.attributes,
    empty
  }

bow_orgName =
  ## (organization name) contains an organizational name. 
  element ns1:orgName {
    bow_macro.phraseSeq,
    bow_att.global.attributes,
    bow_att.datable.attributes,
    bow_att.editLike.attributes,
    bow_att.personal.attributes,
    bow_att.typed.attributes,
    empty
  }

bow_table =
  ## contains text displayed in tabular form, in rows and columns.
  element ns1:table {
    ((bow_model.headLike | bow_model.global)*,
     (bow_row, bow_model.global*)+),
    bow_att.global.attributes,
    ## indicates the number of rows in the table.
    attribute rows { bow_data.count }?,
    ## (columns) indicates the number of columns in each row of the table.
    attribute cols { bow_data.count }?,
    empty
  }

bow_row =
  ## contains one row of a table. 
  element ns1:row {
    bow_cell+,
    bow_att.global.attributes,
    bow_att.tableDecoration.attributes,
    empty
  }

bow_cell =
  ## contains one cell of a table. 
  element ns1:cell {
    bow_macro.paraContent,
    bow_att.global.attributes,
    bow_att.tableDecoration.attributes,
    empty
  }

bow_formula =
  ## contains a mathematical or other formula.
  element ns1:formula {
    (text | bow_model.graphicLike)*,
    bow_att.global.attributes,
    ## specifies the notation used for the content of the element.
    attribute notation { bow_data.enumerated }?,
    empty
  }

bow_notatedMusic =
  ## encodes the presence of music notation in a text
  element ns1:notatedMusic {
    (bow_model.labelLike | bow_model.ptrLike | bow_graphic)*,
    bow_att.global.attributes,
    bow_att.placement.attributes,
    bow_att.typed.attributes,
    empty
  }

bow_figure =
  ## groups elements representing or containing graphic information such as an illustration or
  ##     figure.
  element ns1:figure {
    (bow_model.headLike
     | bow_model.pLike
     | bow_figDesc
     | bow_model.graphicLike
     | bow_model.egLike
     | bow_model.global)*,
    bow_att.global.attributes,
    bow_att.placement.attributes,
    bow_att.typed.attributes,
    empty
  }

bow_figDesc =
  ## (description of figure) contains a brief prose description of the appearance or content
  ## of a graphic figure, for use when documenting an image without
  ## displaying it.
  element ns1:figDesc {
    bow_macro.limitedContent, bow_att.global.attributes, empty
  }

bow_fallback =
  element ns2:fallback { text+ }
start =
  bow_TEI
  | bow_body
  | bow_schemaSpec
  | bow_elementSpec
  | bow_classSpec
  | bow_macroSpec
  | bow_div
