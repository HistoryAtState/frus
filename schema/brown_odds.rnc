        "damageSpan" | "date" | ##  (death) 
        "death" | ##  (decoration description) 
        "decoDesc" | ##  (default feature value) 
        "default" | ##  (default value) 
        "defaultVal" | "if" | "zone" | "surface" | ##  (definition) 
