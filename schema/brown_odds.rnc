bow_moduleSpec =
  ## (module specification) documents the structure, content, and purpose of a single
  ## module, i.e. a named and externally visible group of declarations.
  element ns1:moduleSpec {
    (bow_model.glossLike*, bow_exemplum*, bow_remarks?, bow_listRef*),
    bow_att.global.attributes,
    bow_att.identified.attributes,
    ## type of module to be generated
    attribute type { text }?,
    empty
  }

bow_schemaSpec =
  ## (schema specification) generates a TEI-conformant schema and documentation for it.
  element ns1:schemaSpec {
    (bow_model.glossLike*,
     (bow_model.oddRef | bow_model.oddDecl)*,
     bow_constraintSpec*)
    >> sch:pattern [
         id = "schemaSpec-constraint-nc-prefix"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:schemaSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test = "contains(@prefix,':')"
             "\x{a}" ~
             "                the colon in the value of the prefix= attribute of schemaSpec will cause your schema to be invalid.\x{a}" ~
             "              "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "schemaSpec-constraint-required-modules"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:schemaSpec"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "tei:moduleRef[@key='tei']       and                   tei:moduleRef[@key='core']      and     tei:moduleRef[@key='header']    and     tei:moduleRef[@key='textstructure']"
             "\x{a}" ~
             "                missing one or more of the required modules (tei, core, header, textstructure).\x{a}" ~
             "              "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id = "schemaSpec-constraint-no-outside-specs"
         "\x{a}" ~
         "            "
         sch:rule [
           context =
             "tei:classSpec[not(ancestor::tei:schemaSpec)]                  | tei:elementSpec[not(ancestor::tei:schemaSpec)]           | tei:macroSpec[not(ancestor::tei:schemaSpec)]           "
           "\x{a}" ~
           "                "
           sch:assert [
             test =
               "//tei:schemaSpec//tei:specGrpRef[   substring-after( normalize-space(@target),'#') =   current()/@xml:id]"
             ""
             sch:name [ ]
             " should be in schemaSpec\x{a}" ~
             "		or referred to from within schemaSpec"
           ]
           "\x{a}" ~
           "              "
         ]
         "\x{a}" ~
         "         "
